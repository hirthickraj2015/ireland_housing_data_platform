# Database Configuration
DB_HOST=your-database-host.supabase.co
DB_PORT=5432
DB_NAME=postgres
DB_USER=postgres
DB_PASSWORD=your-secure-password
DB_SCHEMA=public

# Database Connection String (Alternative)
DATABASE_URL=postgresql://user:password@host:port/database

# API Keys (if needed)
# CSO_API_KEY=your-cso-api-key
# ECB_API_KEY=your-ecb-api-key

# Scraping Configuration
DAFT_BASE_URL=https://www.daft.ie
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36

# Data Refresh Settings
SCRAPE_DELAY_SECONDS=2
MAX_RETRIES=3
TIMEOUT_SECONDS=30

# Environment
ENVIRONMENT=development
LOG_LEVEL=INFO

# Power BI (if using API)
# POWERBI_WORKSPACE_ID=your-workspace-id
# POWERBI_DATASET_ID=your-dataset-id
# POWERBI_CLIENT_ID=your-client-id
# POWERBI_CLIENT_SECRET=your-client-secret
# POWERBI_TENANT_ID=your-tenant-id
